<%= form_with(model: @schedule, local: true) do |form| %>

  <div class="title">
    <%= form.label :title %>
    <%= form.text_field :title %>
  </div>

  <div class="time">
    <%= form.label :start_time %>
    <%= form.datetime_select :start_time %>
  </div>

  <div class="place">
    <%= form.label :place %>
    <%= form.text_field :place %>
  </div>

  <div class="person">
    <%= form.label :person %>
    <%= form.text_field :person %>
  </div>

  <div class="practice">
    <%= form.label :practice %>
    <%= form.text_field :practice %>
  </div>

  <div class="others">
    <%= form.label :others %>
    <%= form.text_field :others %>
  </div>

  <div class="submit">
    <%= form.submit %>
  </div>

<% end %>